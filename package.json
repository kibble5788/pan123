{
  "name": "@ked3/pan123-sdk-v2",
  "version": "2.0.0",
  "description": "123网盘SDK TypeScript版本，支持文件上传、下载、管理等功能",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kibble5788/pan123.git"
  },
  "bugs": {
    "url": "https://github.com/kibble5788/pan123/issues"
  },
  "homepage": "https://github.com/kibble5788/pan123#readme",
  "keywords": [
    "123pan",
    "cloud-storage",
    "file-upload",
    "file-download",
    "sdk",
    "api",
    "typescript"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "build:dev": "tsc --watch",
    "dev": "tsc --watch",
    "test": "node  ./test/index.js",
    "debug": "node --loader ts-node/esm ./test/index.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/crypto-js": "^4.2.1",
    "@types/node": "^20.10.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "https-proxy-agent": "^7.0.6",
    "tslib": "^2.8.1"
  }
}
